<PropertyList>

  <path>hydPressMain.ac</path>
  <animation>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>Face</object-name>
    <object-name>Needle</object-name>
    <emission>
	   <red>0.028</red>
       <green>0.014</green>
       <blue>0.007</blue>
       <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <!---->
  <animation>
    <type>rotate</type>
    <object-name>Needle</object-name>
    <property>/surface-positions/flap-pos-norm</property>
    <interpolation>
      <entry><ind>    0 </ind><dep> 130 </dep></entry>
	  <entry><ind> 1000 </ind><dep>  90 </dep></entry>
      <entry><ind> 2000 </ind><dep> 180 </dep></entry>
    </interpolation>
    <axis>
      <x> -1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>

</PropertyList>

