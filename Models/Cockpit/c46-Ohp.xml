   <!-- 1A GAUGES --><!--Done, second "amp" source needed-->
   <animation>
    <type>rotate</type>
    <object-name>O1VoltsNeedle</object-name>
    <property>/systems/electrical/volts</property>
    <factor>-2.6</factor>
	<offset-deg>40</offset-deg>
    <center>
      <x-m>-10.524</x-m>
      <y-m>-0.0210</y-m>
      <z-m>0.3612</z-m>
    </center>
    <axis>
      <x>0.309</x>
      <y>0.0</y>
      <z>-1.0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>O1amps1Needle</object-name>
    <property>/systems/electrical/engine[0]/ammeter</property>
    <factor>-.26</factor>
	<condition>
	  <greater-than>
	    <property>/systems/electrical/engine[0]/ammeter</property>
		<value>-50</value>
	  </greater-than>
	</condition>
	<offset-deg>40</offset-deg>
    <center>
      <x-m>-10.524</x-m>
      <y-m>0.0730</y-m>
      <z-m>0.3612</z-m>
    </center>
    <axis>
      <x>0.309</x>
      <y>0.0</y>
      <z>-1.0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>O1Amps2Needle</object-name>
    <property>/systems/electrical/engine[1]/ammeter</property>
    <factor>-.26</factor>
	<condition>
	  <greater-than>
	    <property>/systems/electrical/engine[1]/ammeter</property>
		<value>-50</value>
	  </greater-than>
	</condition>
	<offset-deg>40</offset-deg>
    <center>
      <x-m>-10.524</x-m>
      <y-m>0.1676</y-m>
      <z-m>0.3612</z-m>
    </center>
    <axis>
      <x>0.309</x>
      <y>0.0</y>
      <z>-1.0</z>
    </axis>
  </animation>
  
  <animation>
    <type>material</type>
    <object-name>O1gauges</object-name>
