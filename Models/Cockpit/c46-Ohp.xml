   <path>OHP.ac</path>
   
   <effect>
    <inherits-from>Aircraft/c46/Models/Effects/interior/interior-glass-reflection-panel-front</inherits-from>
    <object-name>O1Glass</object-name>
   </effect>
   
   <!--#########################################################################-->
   <!-- GAUGES -->
   <animation>
    <type>rotate</type>
    <object-name>O1VoltsNeedle</object-name>
    <property>/systems/electrical/volts</property>
    <factor>-2.6</factor>
	<offset-deg>40</offset-deg>
    <center>
      <x-m>-10.524</x-m>
      <y-m>-0.0210</y-m>
      <z-m>0.3612</z-m>
    </center>
    <axis>
      <x>0.309</x>
      <y>0.0</y>
      <z>-1.0</z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>O1amps1Needle</object-name>
    <property>/systems/electrical/amps</property>
    <factor>-.26</factor>
	<condition>
	  <greater-than>
	    <property>/systems/electrical/amps</property>
		<value>-50</value>
	  </greater-than>
	</condition>
	<offset-deg>40</offset-deg>
    <center>
      <x-m>-10.524</x-m>
      <y-m>0.0730</y-m>
      <z-m>0.3612</z-m>
    </center>
    <axis>
      <x>0.309</x>
      <y>0.0</y>
      <z>-1.0</z>
    </axis>
  </animation>
  <animation>
    <type>rotate</type>
    <object-name>O1Amps2Needle</object-name>
    <property>/systems/electrical/amps</property>
    <factor>-.26</factor>
	<condition>
	  <greater-than>
	    <property>/systems/electrical/amps</property>
		<value>-50</value>
	  </greater-than>
	</condition>
	<offset-deg>40</offset-deg>
    <center>
      <x-m>-10.524</x-m>
      <y-m>0.1676</y-m>
      <z-m>0.3612</z-m>
    </center>
    <axis>
      <x>0.309</x>
      <y>0.0</y>
      <z>-1.0</z>
    </axis>
  </animation>
  
  <animation>
  <type>material</type>
  <object-name>O1gauges</object-name>
    <emission>
        <red>0.028</red>
        <green>0.014</green>
        <blue>0.007</blue>
        <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>
  
    <!--#########################################################################--> 
    <!--RHEOSTATS-->
    <!--<object-name>O1RhCompass</object-name>
        <object-name>O1RhInstPanel</object-name>
        <object-name>O1RhInstruments</object-name>
        <object-name>O1RhOverhead</object-name>-->
    <animation>
      <type>knob</type>
      <object-name>O1RhInstPanel</object-name>
      <property>/controls/lighting/panel-norm</property>	
      <factor>-300</factor>
      <offset-deg>150</offset-deg>
      <center>
        <x-m>-10.4379</x-m><y-m>-0.18844</y-m><z-m>0.395229</z-m>
      </center>
      <axis>
        <x>0.309</x><y>0</y><z>-1</z>
      </axis>
      <action>
        <binding>
          <command>property-adjust</command>
          <property>/controls/lighting/panel-norm</property>
          <factor>0.2</factor>
          <min>0</min>
          <max>1</max>                
          <wrap>false</wrap>
          <drag-scale-px>2</drag-scale-px>
		</binding>
      </action>
      <hovered>
        <binding>
          <command>set-tooltip</command>
          <tooltip-id>rhinstpan</tooltip-id>
		  <property>/controls/lighting/panel-norm</property>
		  <measure-text>Panel: 100%</measure-text>
          <label>Panel: %d%%</label>
		  <mapping>percent</mapping>
        </binding>
      </hovered>
    </animation>
    
    <animation><!--"Compass" switch here for Radio-->
      <type>knob</type>
      <object-name>O1RhCompass</object-name>
      <property>/controls/lighting/radio-norm</property>	
      <factor>-300</factor>
      <offset-deg>150</offset-deg>
      <center>
        <x-m>-10.5079</x-m><y-m>-0.190329</y-m><z-m>0.371103</z-m>
      </center>
      <axis>
        <x>0.309</x><y>0</y><z>-1</z>
      </axis>
      <action>
        <binding>
          <command>property-adjust</command>
          <property>/controls/lighting/radio-norm</property>
          <factor>0.2</factor>
          <min>0</min>
          <max>1</max>                
          <wrap>false</wrap>
		</binding>
      </action>
      <hovered>
        <binding>
          <command>set-tooltip</command>
          <tooltip-id>rhradios</tooltip-id>
		  <property>/controls/lighting/radio-norm</property>
		  <measure-text>Navigation Radio: 100%</measure-text>
          <label>Navigation/Radios: %d%%</label>
		  <mapping>percent</mapping>
        </binding>
      </hovered>
    </animation>
    
    <animation>
