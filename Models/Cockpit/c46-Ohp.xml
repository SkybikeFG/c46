   <!-- MAGNETOS  PANEL-->
   <animation>
      <type>knob</type>
      <object-name>O1Mag1</object-name>
      <property>/controls/engines/engine/magnetos</property>	
	  <factor>-35</factor>
      <center>
        <x-m>-10.3167</x-m><y-m>-0.033928</y-m><z-m>0.43094</z-m>
      </center>
      <axis>
        <x>0.309</x><y>0</y><z>-1</z>
      </axis>
      <action>
        <binding>
          <command>property-adjust</command>
          <property>/controls/engines/engine/magnetos</property>
          <factor>1</factor>
          <min>0</min>
          <max>3</max>                
          <wrap>false</wrap>
		</binding>
      </action>
      <hovered>
        <binding>
          <command>set-tooltip</command>
          <tooltip-id>mag-left</tooltip-id>
		  <property>/controls/engines/engine/magnetos</property>
		  <measure-text>Left Magnetos: 1 only</measure-text>
          <label>Left Magnetos: %s</label>
		  <mapping>nasal</mapping>
		 <script>modes=['off', 'L only', 'R only', 'both'];
		         return modes[arg[0]];</script>
        </binding>
      </hovered>
    </animation>
    <animation>
      <type>knob</type>
      <object-name>O1Mag2</object-name>
      <property>/controls/engines/engine[1]/magnetos</property>	
	  <factor>35</factor>
      <center>
        <x-m>-10.3167</x-m><y-m>0.016426</y-m><z-m>0.43094</z-m>
      </center>
      <axis>
        <x>0.309</x><y>0</y><z>-1</z>
