    <!--1B RHEOSTATS-->
    <!--Done, compass needed-->
    <animation>
      <type>knob</type>
      <object-name>O1RhInstPanel</object-name>
      <property>/controls/lighting/panel-norm</property>	
      <factor>-300</factor>
      <offset-deg>150</offset-deg>
      <center>
        <x-m>-10.4379</x-m><y-m>-0.18844</y-m><z-m>0.395229</z-m>
      </center>
      <axis>
        <x>0.309</x><y>0</y><z>-1</z>
      </axis>
      <action>
        <binding>
          <command>property-adjust</command>
          <property>/controls/lighting/panel-rheo</property>
          <factor>0.2</factor>
          <min>0</min>
          <max>1</max>                
          <wrap>false</wrap>
          <drag-scale-px>2</drag-scale-px>
		</binding>
        <binding>
          <command>nasal</command>
          <script>if(getprop("/controls/circuit-breakers/intlt")==0){
	            gui.popupTip("Check Circuit-Breaker for Instrument Panel Light", 5);}</script>
        </binding>
      </action>
      <hovered>
        <binding>
          <command>set-tooltip</command>
          <tooltip-id>rhinstpan</tooltip-id>
		  <property>/controls/lighting/panel-rheo</property>
		  <measure-text>Panel: 100%</measure-text>
          <label>Panel: %d%%</label>
		  <mapping>percent</mapping>
        </binding>
      </hovered>
    </animation>
    
