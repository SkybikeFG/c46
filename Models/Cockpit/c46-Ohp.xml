	<!--SWITCHES row "magnetos"-->
			
  <animation>
    <type>rotate</type>
    <object-name>O1NavigLights</object-name>
    <property>/controls/lighting/nav-int</property>
	<interpolation>
	  <entry><ind>0</ind><dep>-25</dep></entry>
	  <entry><ind>0.5</ind><dep>-45</dep></entry>
	  <entry><ind>1</ind><dep>-60</dep></entry>
	  <entry><ind>1.5</ind><dep>-25</dep></entry>
	  <entry><ind>2</ind><dep> 10</dep></entry>
    </interpolation>  
    <factor>1</factor>
	<offset-deg>0</offset-deg>
    <center>
      <x-m>-10.315</x-m>
      <y-m>0</y-m>
      <z-m>0.438</z-m>
    </center>
    <axis>
      <x>0.0</x>
      <y>1.0</y>
      <z>0.0</z>
    </axis>
  </animation>
  <animation>
      <type>knob</type>
      <object-name>O1NavigLights</object-name>
	  <!--object-name>O1NavigLightsArea</object-name-->
      <value>0</value>	
      <action>
        <binding>
          <command>property-adjust</command>
          <property>/controls/lighting/nav-int</property>
		  <step>1</step>
          <min>0</min>
          <max>3</max>
          <wrap>true</wrap>
          <drag-direction>vertical</drag-direction>
		</binding>
      </action>
	<hovered>
      <binding>
        <command>set-tooltip</command>
        <label>Navigation Lights: %s</label>
        <tooltip-id>navlgt</tooltip-id>
	    <mapping>nasal</mapping>
        <property>/controls/lighting/nav-int</property>
		<script>ausgabe=['OFF','FLASH','STEADY']; return ausgabe[arg[0]];</script>
      </binding>
    </hovered>
  </animation>
  
	  
