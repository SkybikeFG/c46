        </equals>
	  </and>
    </condition>
    <emission>
	  <red>1</red>
      <green>1</green>
      <blue>1</blue>
      <factor-prop>controls/lighting/annun-on-norm</factor-prop>
    </emission>
  </animation>
  
  
