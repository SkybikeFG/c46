 <!--Lower pedestal rotary lever section ("P3"), centre [-10.06; 0; -0.73], angle -20 to 75 -->
   <animation>
    <type>rotate</type>
    <object-name>P3Flap</object-name>
    <property>controls/flight/flaps</property>
    <interpolation>
      <entry><ind>   0</ind><dep>-45</dep></entry>
      <entry><ind>0.25</ind><dep>  0</dep></entry>
      <entry><ind>0.43</ind><dep> 20</dep></entry>
      <entry><ind>0.72</ind><dep> 30</dep></entry>
      <entry><ind>   1</ind><dep> 45</dep></entry>
    </interpolation>
    <center>
