<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>
  
  <path>general_pilot.ac</path>
  
  <animation>
    <!-- Objets opaques -->
    <object-name>tete</object-name>
    <object-name>visiere</object-name>
    <object-name>corps</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>manche</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
    <object-name>jambeG</object-name>
    <object-name>jambeD</object-name>
    <object-name>piedG</object-name>
    <object-name>piedD</object-name>
    
    <!-- Objets transparents -->
  </animation>
  
  <animation>
    <type>select</type>
    <object-name>tete</object-name>
    <object-name>visiere</object-name>
    <object-name>corps</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>manche</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
    <object-name>jambeG</object-name>
    <object-name>jambeD</object-name>
    <object-name>piedG</object-name>
    <object-name>piedD</object-name>
    <condition>
      <property>sim/model/config/show-pilot</property>
    </condition>
  </animation>
  
  <!-- Civilian red version -->
  <animation>
    <type>material</type>
    <object-name>tete</object-name>
    <object-name>visiere</object-name>
    <object-name>corps</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>manche</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
    <object-name>jambeG</object-name>
    <object-name>jambeD</object-name>
    <object-name>piedG</object-name>
    <object-name>piedD</object-name> 
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>civilian-red</value>
      </equals>
    </condition>
    <texture>general_pilot.png</texture>
  </animation>
  
  <!-- Civilian gray version -->
  <animation>
    <type>material</type>
    <object-name>tete</object-name>
    <object-name>visiere</object-name>
    <object-name>corps</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>manche</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
    <object-name>jambeG</object-name>
    <object-name>jambeD</object-name>
    <object-name>piedG</object-name>
    <object-name>piedD</object-name> 
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>civilian-gray</value>
      </equals>
    </condition>
    <texture>Liveries/general_pilot_gray.png</texture>
  </animation>
  
  <!-- Military version -->
  <animation>
    <type>material</type>
    <object-name>tete</object-name>
    <object-name>visiere</object-name>
    <object-name>corps</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>manche</object-name>
    <object-name>cuisseG</object-name>
    <object-name>cuisseD</object-name>
    <object-name>jambeG</object-name>
    <object-name>jambeD</object-name>
    <object-name>piedG</object-name>
    <object-name>piedD</object-name> 
    <condition>
      <equals>
        <property>sim/model/config/version</property>
        <value>military</value>
      </equals>
    </condition>
    <texture>Liveries/general_pilot_military.png</texture>
  </animation>
  
  <animation>
    <name>AnimeTete</name>
    <object-name>tete</object-name>
    <object-name>visiere</object-name>
  </animation>
  
  <!-- Visière invisible depuis le cockpit -->
  <animation>
    <type>select</type>
    <object-name>visiere</object-name>
    <condition>
      <greater-than>
        <property>sim/current-view/view-number</property>
        <value>0</value>
      </greater-than>
    </condition>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>AnimeTete</object-name>
    <property>controls/flight/elevator</property>
    <factor> -2 </factor> 
    <center>
      <x-m> 0.366 </x-m>
      <y-m> 0.005 </y-m>
      <z-m> 0.411 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>AnimeTete</object-name>
    <property>controls/flight/rudder</property>
    <factor> -25 </factor>
    <center>
      <x-m> 0.366 </x-m>
      <y-m> 0.005 </y-m>
      <z-m> 0.411 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>AnimeTete</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <factor> -10 </factor>
    <center>
      <x-m> 0.366 </x-m>
      <y-m> 0.005 </y-m>
      <z-m> 0.411 </z-m>
    </center>
    <axis>
      <x> 1 </x>
      <y> 0 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!-- Tirer sur le manche-->
  <!-- coude-->
  <animation>
    <type>rotate</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <property>controls/flight/elevator</property>
    <interpolation>
      <entry><ind> -1 </ind><dep>  20 </dep></entry>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  1 </ind><dep> -20 </dep></entry>
    </interpolation> 
    <axis>
      <x1-m>  0.200 </x1-m>
      <y1-m> -0.200 </y1-m>
      <z1-m>  0.114 </z1-m>
      <x2-m>  0.185 </x2-m>
      <y2-m>  0.208 </y2-m>
      <z2-m>  0.150 </z2-m>
    </axis>
  </animation>
  
  <!--épaule-->
  <animation>
    <type>rotate</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <property>controls/flight/elevator</property>
    <interpolation>
      <entry><ind> -1 </ind><dep> -18 </dep></entry>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  1 </ind><dep>  17 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.417 </x-m>
      <y-m> 0.000  </y-m>
      <z-m> 0.325 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!--Bassin-->
  <animation>
    <type>rotate</type>
    <object-name>avantbrasG</object-name>
    <object-name>avantbrasD</object-name>
    <object-name>brasG</object-name>
    <object-name>brasD</object-name>
    <object-name>corps</object-name>
    <object-name>tete</object-name>
    <object-name>visiere</object-name>
    <property>controls/flight/elevator</property>
    <factor> 5 </factor>
    <center>
      <x-m>  0.365 </x-m>
      <y-m>  0.000 </y-m>
      <z-m> -0.042 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <!-- Tourner le volant-->
  <!--épaule gauche-->
  <animation>
    <type>rotate</type>
    <object-name>avantbrasG</object-name>
    <object-name>brasG</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -1 </ind><dep> 8 </dep></entry>
      <entry><ind>  0 </ind><dep> 0 </dep></entry>
      <entry><ind>  1 </ind><dep> 3 </dep></entry>
    </interpolation>
    <center>
      <x-m> 0.425 </x-m>
      <y-m> 0.190 </y-m>
      <z-m> 0.344 </z-m>
    </center>
    <axis>
      <x>  0.8 </x>
      <y> -1.2 </y>
      <z>  0.0 </z>
    </axis>
  </animation>
  
  <!--épaule droite-->
  <animation>
    <type>rotate</type>
    <object-name>avantbrasD</object-name>
    <object-name>brasD</object-name>
    <property>surface-positions/left-aileron-pos-norm</property>
    <interpolation>
      <entry><ind> -1 </ind><dep>  -2 </dep></entry>
      <entry><ind>  0 </ind><dep>   0 </dep></entry>
      <entry><ind>  1 </ind><dep> -10 </dep></entry>
    </interpolation>
    <center>
      <x-m>  0.400   </x-m>
      <y-m> -0.165  </y-m>
      <z-m>  0.315  </z-m>
    </center> 
    <axis>
      <x> 0.8 </x>
      <y> 1.2 </y>
      <z> 0.0 </z>
    </axis>
  </animation>
  
  <!-- Pédales--> 
  <!--gauche-->
  <animation>
    <type>rotate</type>
    <object-name>cuisseG</object-name>
    <object-name>jambeG</object-name>
    <object-name>piedG</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 5 </factor>
    <center>
      <x-m>  0.318 </x-m>
      <y-m>  0.140 </y-m>
      <z-m> -0.077 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>jambeG</object-name>
    <object-name>piedG</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 14 </factor>
    <center>
      <x-m> -0.120 </x-m>
      <y-m>  0.080 </y-m>
      <z-m> -0.047 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>piedG</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 25 </factor>
    <center>
      <x-m> -0.335 </x-m>
      <y-m>  0.088 </y-m>
      <z-m> -0.286 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <!--droite-->
  <animation>
    <type>rotate</type>
    <object-name>cuisseD</object-name>
    <object-name>jambeD</object-name>
    <object-name>piedD</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 5 </factor>
    <center>
      <x-m>  0.318 </x-m>
      <y-m> -0.140 </y-m>
      <z-m> -0.077 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>jambeD</object-name>
    <object-name>piedD</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 14 </factor>
    <center>
      <x-m> -0.120 </x-m>
      <y-m> -0.080 </y-m>
      <z-m> -0.047 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>piedD</object-name>
    <property>/controls/flight/rudder</property>
    <factor> 25 </factor>
    <center>
      <x-m> -0.335 </x-m>
      <y-m>  0.088 </y-m>
      <z-m> -0.286 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <!-- Frein -->
  <animation>
    <type>rotate</type>
    <object-name>piedD</object-name>
    <property>/controls/gear/brake-right</property>
    <factor> 25 </factor>
    <center>
      <x-m> -0.335 </x-m>
      <y-m>  0.088 </y-m>
      <z-m> -0.286 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>piedG</object-name>
    <property>/controls/gear/brake-left</property>
    <factor> 25 </factor>
    <center>
      <x-m> -0.335 </x-m>
      <y-m>  0.088 </y-m>
      <z-m> -0.286 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
</PropertyList>
