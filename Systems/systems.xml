<PropertyList>
  <!-- *** Temporary Electrical System *** -->
  <electrical>
    <name>electrical</name>
    <number>0</number>
    <path>Aircraft/Generic/generic-electrical.xml</path>
  </electrical>
  
   <!-- *** Pitot System *** -->
  <pitot>
    <name>pitot</name>
    <number>0</number>
	<stall-deg>60</stall-deg>
  </pitot>
  
  <pitot>
    <name>pitot</name>
    <number>1</number>
	<stall-deg>60</stall-deg>
  </pitot>

  <!-- *** Static System *** -->
  <static>
    <name>static</name>
    <number>0</number>
    <tau>0.1</tau>
	<type>2</type> <!-- Static port on pitot tube -->
	<error-factor>0.5</error-factor>
  </static>
  
  <static>
    <name>static</name>
    <number>1</number>
    <tau>0.1</tau>
	<type>2</type> <!-- Static port on pitot tube -->
	<error-factor>0.5</error-factor>
  </static>
  
  <static>
    <name>static</name>
    <number>2</number>
    <tau>0.05</tau>
	<type>0</type> <!-- Static port on left wing -->
	<error-factor>0.7</error-factor> <!-- POH notes that alternate static source cause 300' higher altimeter readings -->
  </static>
  
  <!-- *** Vacuum System *** -->
  <vacuum>
    <name>vacuum</name>
    <number>0</number>
    <rpm>/engines/engine[0]/rpm</rpm>
    <scale>1.0</scale>
  </vacuum>

  <vacuum>
    <name>vacuum</name>
    <number>1</number>
    <rpm>/engines/engine[1]/rpm</rpm>
    <scale>1.0</scale>
  </vacuum>

  


    

</PropertyList>

